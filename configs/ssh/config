# IndustrialBrothers Internal network
Host ib-*
    Hostname %h.industrialbrothers.com
	IdentityFile ~/.ssh/industrialbrothers_id_ed25519
    RequestTTY force
    User epratten

Host *.industrialbrothers.com
	IdentityFile ~/.ssh/industrialbrothers_id_ed25519
	
